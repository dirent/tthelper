<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<head>
    <title>Meldung Pokalmannschaft</title>
    <style>
    #verein {
    	width:250px;
    }
    
    #wettbewerb {
    	width:250px;
    }
    
    #name {
    	width:250px;
    }
    
    #pokalanzahl {
    	width:50px;
    	text-align:left;
    }
    
    #bemerkung {
    	width:250px;
    }
    
    .t-beaneditor {
    	width:600px;
    }
    
    input.t-beaneditor-submit {
    	float:right;
    	position:relative;
    	top:-2em;
    	left:-1em;
    }
    </style>
</head>

<body>
<h1>Gemeldete Pokalmannschaften</h1>

<t:grid source="gemeldetePokalMannschaften" row="pm" />

<h1>Neue Pokalmannschaft melden</h1>

<t:beaneditform t:id="CreateMannschaft" submitlabel="Ok" object="mannschaft">
<t:parameter name="verein">
	<t:label for="verein"/>
	<t:select t:id="verein" disabled="true" value="mannschaft.verein" />
</t:parameter>
<t:parameter name="bemerkung">
	<t:label for="bemerkung"/>
	<t:textarea t:id="bemerkung" rows="4" value="mannschaft.bemerkung" />
</t:parameter>
</t:beaneditform>

<p style="width:100%; text-align:right; margin-right:1em; padding-bottom:1em; margin-bottom:1em; border-bottom: 1px dotted gray;"><t:pagelink page="Start">Home</t:pagelink></p>

</body>
</html>
