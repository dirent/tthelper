<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<head>
    <title>Ranglistenmeldung</title>
    <style>
	.t-data-grid {
    	font-size:8pt;
    }

    #verein {
    	width:250px;
    }

	#name {
		width:250px;
	}
	
	#birthdate {
		width:250px;
	}
	
	#konkurrenz {
		width:250px;
	}
	
	.t-beaneditor {
    	width:600px;
    }
    
    input.t-beaneditor-submit {
    	float:right;
    	position:relative;
    	top:-2em;
    	left:-1em;
    }
    </style>
</head>

<body>
<h1>Gemeldete Spieler</h1>

<t:block id="empty">Es sind noch keine Spieler gemeldet.</t:block>

<t:grid source="gemeldeteRanglistenSpieler" row="rs" model="model" empty="block:empty">
<t:parameter name="teilnahmeBezirksranglisteCell">
	<t:output t:format="booleanFormat" t:value="rs.teilnahmeBezirksrangliste" />
</t:parameter>
<t:parameter name="teilnahmeBezirksmeisterschaftenCell">
	<t:output t:format="booleanFormat" t:value="rs.teilnahmeBezirksmeisterschaften" />
</t:parameter>
<t:parameter name="deletecell">
	<t:actionlink t:id="delete" context="rs.id">LÃ¶schen</t:actionlink>
</t:parameter>
</t:grid>


<h1>Ranglistenmeldung</h1>

<t:beaneditform t:id="spieler" submitlabel="Ok" object="spieler">
<t:parameter name="verein">
	<t:label for="verein"/>
	<t:select t:id="verein" disabled="true" value="spieler.verein" />
</t:parameter>
</t:beaneditform>

<p style="width:100%; text-align:right; margin-right:1em; padding-bottom:1em; margin-bottom:1em; border-bottom: 1px dotted gray;"><t:pagelink page="Start">Home</t:pagelink></p>

</body>
</html>
